---
import { SEOProps } from "./shared";

export type Props = Pick<SEOProps, "peopleData">;
const { peopleData } = Astro.props;
---

{
	peopleData?.length ? (
		<>
			<meta property="profile:firstName" content={peopleData[0].firstName} />
			<meta property="profile:lastName" content={peopleData[0].lastName} />
			<meta property="profile:username" content={peopleData[0].id} />
		</>
	) : undefined
}
